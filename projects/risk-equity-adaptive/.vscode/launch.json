// projects/risk-equity-adaptive/.vscode/launch.json
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      // Set the path of this subproject as the current workspace
      "cwd": "${workspaceFolder}",
      "env": {
        // set PYTHONPATH as the current workspaceï¼Œletting Python to find packages
        "PYTHONPATH": "${workspaceFolder}/envs/python.exe"
      },
      "justMyCode": false
    }
  ]
}
